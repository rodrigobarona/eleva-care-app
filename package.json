{
  "name": "eleva-care-app",
  "version": "0.3.1",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev --port 3000",
    "build": "NODE_NO_WARNINGS=1 next build",
    "build:force": "NODE_NO_WARNINGS=1 NEXT_SKIP_TYPESCRIPT_CHECK=1 next build",
    "start": "NODE_NO_WARNINGS=1 next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate": "drizzle-kit generate",
    "migrate": "drizzle-kit migrate",
    "push": "drizzle-kit push",
    "studio": "drizzle-kit studio",
    "auditdb:generate": "drizzle-kit generate --config audit.config.ts",
    "auditdb:migrate": "drizzle-kit migrate --config audit.config.ts",
    "migrate:profile-published": "tsx scripts/rename-profile-published.ts",
    "qstash:setup": "node --input-type=module scripts/setup-qstash.js",
    "qstash:check": "node --input-type=module scripts/check-qstash-env.js",
    "qstash:debug": "node --input-type=module scripts/debug-env-loading.js",
    "qstash:env": "node --input-type=module scripts/setup-env-vars.js",
    "qstash:test": "node --input-type=module scripts/test-qstash-connection.js",
    "qstash:update": "tsx scripts/update-qstash-schedules.ts",
    "fix-connect-payouts": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-specifier-resolution=node tsx scripts/fix-connect-payout-schedules.ts",
    "postbuild": "NODE_NO_WARNINGS=1 NODE_OPTIONS=--experimental-specifier-resolution=node tsx scripts/update-qstash-schedules.ts || echo 'QStash setup skipped'",
    "cleanup:duplicates": "NODE_NO_WARNINGS=1 tsx scripts/cleanup-duplicate-reservations.ts",
    "cleanup:cache": "node -r dotenv/config node_modules/.bin/tsx scripts/cleanup-payment-rate-limit-cache.ts",
    "test:keep-alive": "node -r dotenv/config node_modules/.bin/tsx scripts/test-keep-alive-cache-cleanup.ts",
    "test": "jest",
    "test:critical": "jest tests/api/create-payment-intent.test.ts tests/api/webhooks/ tests/api/getValidTimesFromSchedule.test.ts --passWithNoTests",
    "test:deprecated": "jest tests/deprecated/ --passWithNoTests",
    "test:webhooks": "jest tests/api/webhooks/ --passWithNoTests",
    "test:payment": "jest tests/api/create-payment-intent.test.ts --passWithNoTests",
    "test:scheduling": "jest tests/api/getValidTimesFromSchedule.test.ts --passWithNoTests",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "setup:posthog-dashboards": "node -r dotenv/config scripts/setup-posthog-dashboards.js",
    "posthog:dashboards": "npm run setup:posthog-dashboards",
    "test:posthog": "node -r dotenv/config scripts/test-posthog-connection.js",
    "test:posthog-env": "node -r dotenv/config scripts/test-env-visibility.js",
    "validate:posthog-permissions": "node -r dotenv/config scripts/validate-posthog-permissions.js",
    "novu:sync": "npx novu@latest sync --bridge-url \"$NOVU_BRIDGE_URL\" --secret-key \"$NOVU_SECRET_KEY\" --api-url \"$NOVU_BASE_URL\"",
    "novu:sync:dev": "bash -c 'source .env && npx novu@latest sync --bridge-url \"$NGROK_URL/api/novu\" --secret-key \"$NOVU_SECRET_KEY\" --api-url \"$NOVU_BASE_URL\"'",
    "novu:sync:dev:auto": "bash -c 'NGROK_URL=$(curl -s http://localhost:4040/api/tunnels | jq -r \".tunnels[0].public_url\" 2>/dev/null || echo \"http://localhost:3000\") && source .env && npx novu@latest sync --bridge-url \"$NGROK_URL/api/novu\" --secret-key \"$NOVU_SECRET_KEY\" --api-url \"$NOVU_BASE_URL\"'",
    "novu:sync:prod": "bash -c 'source .env && npx novu@latest sync --bridge-url \"https://eleva.care/api/novu\" --secret-key \"$NOVU_SECRET_KEY_PROD\" --api-url \"$NOVU_BASE_URL\"'",
    "novu:sync:preview": "bash -c 'source .env && npx novu@latest sync --bridge-url \"$VERCEL_PREVIEW_URL/api/novu\" --secret-key \"$NOVU_SECRET_KEY\" --api-url \"$NOVU_BASE_URL\"'",
    "novu:diagnostics": "node scripts/test-novu-diagnostics.js",
    "qstash:schedule": "node scripts/qstash-manager.js schedule",
    "qstash:list": "node scripts/qstash-manager.js list",
    "qstash:cleanup": "node scripts/qstash-manager.js cleanup",
    "qstash:stats": "node scripts/qstash-manager.js stats",
    "analyze": "ANALYZE=true pnpm build",
    "analyze:server": "ANALYZE=true BUNDLE_ANALYZE=server pnpm build",
    "analyze:browser": "ANALYZE=true BUNDLE_ANALYZE=browser pnpm build",
    "build:analyze": "cross-env ANALYZE=true pnpm build",
    "build:debug": "NODE_OPTIONS='--inspect' pnpm build",
    "build:profile": "pnpm build --profile",
    "build:memory": "pnpm build --experimental-debug-memory-usage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "type-check": "tsc --noEmit",
    "test:email": "tsx scripts/test-email-templates.mjs",
    "email:dev": "email dev",
    "email:export": "email export --dir emails --outDir ./.react-email",
    "email:preview": "email dev --dir emails --port 3001"
  },
